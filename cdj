    $(".search .advance span:not('.showAdv')").click(function (e) {
        var serTar = $(e.target);
        if (serTar.hasClass('showAdv')) {
            $('.search-focus').fadeOut();
            $(this).removeClass('showAdv');
        } else {
            $(this).addClass('showAdv');
            $(this).parents('.search-field').next('.search-focus').fadeIn();
            $(this).parents('.search').bind("clickoutside", function (e) {
                $('.search-focus').fadeOut();
                $('.search').find('.showAdv').removeClass('showAdv');
            });
        }
    });

   
