                name: "ProductDeal",
                url: "Deal/{id}",
                defaults: new { controller = "Deal", action = "Index" }
            );

            routes.MapRoute(
                name: "Default",
                url: "{controller}/{action}/{id}",
                defaults: new { controller = "Home", action = "Index", id = UrlParameter.Optional }
            );
        }
    }
