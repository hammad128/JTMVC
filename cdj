    $(".customdatepicker").datepicker({
        dateFormat: "D m/d/yy",
        changeMonth: false,
        changeYear: false,
        beforeShowDay: function (date) {
            var search = date.getFullYear() + '/' + (date.getMonth() + 1) + '/' + date.getDate();
            if (search in dates) {
                return [true, 'highlight-font-color', (dates[search] || '')];
            }
            return [true, '', ''];
        }
    });

    $('.dealer').on('change', this, function () {
        var getName = $(this).find('option:selected').attr('class');
        $('.dealer-packages').removeClass('active')
        $('.box1-' + getName).addClass('active');
    });

    $('.selectmonths li').on('click', this, function () {
        $('.selectmonths li').removeClass('active');
        $(this).addClass('active');
        var date = $(".customdatepicker").datepicker('getDate');
        var month = parseInt($(this).attr('data-selectMonth'));
        date.setMonth(month);
        $(".customdatepicker").datepicker("setDate", date);
    });

    setActiveMonth();
    $(document).on('click', '.ui-datepicker-next', function () {
        setActiveMonth()
    });
    $(document).on('click', '.ui-datepicker-prev', function () {
        setActiveMonth()
    });

