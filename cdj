    //autocomp('.currencyCountry', currencyCountry);

    var slideshows = $('.cycle-slideshow:not(".main")').on('cycle-next cycle-prev', function (e, opts) {
        slideshows.not(this).cycle('goto', opts.currSlide);
    });

    $('body').on('click', '.cycle-slideshow .cycle-slide', function () {
        var curInd = $(this).index();
        $(this).closest('.wrapCycle').find('.cycle-slideshow.cycle1').cycle('goto', curInd);
        $(this).closest('.cycle-slideshow').cycle('goto', curInd);
        console.log(curInd);
    });

    setInterval(function () {
        var e = $(".activethis").removeClass("activethis");
        if (e.next() && e.next().length) {
            e.next().addClass("activethis")
        } else {
            e.siblings(":first").addClass("activethis")
        }
    }, 2e3);

    $(".faqaccordion li a").on('click', function () {
        var showBox = $(this).next('.innerdata');
        $(".faqaccordion li").removeClass("activeit");
        if (showBox.is(":visible")) {
            showBox.slideUp();
        } else {
            $('.innerdata').slideUp();
            $(this).parent().addClass("activeit");
            showBox.slideDown(function () {
                $("html,body").animate({
                    scrollTop: $(this).parent().offset().top
                }, 'slow');
            });
        }
    });



    $(".back-to-top, .faqlist li a  , .details-section li a").click(function () {
        var e = $(this.hash);
        goToByScroll(e);
        return false
    });

