using CarDealsJapanCore.Common;
using CarDealsJapanCore.Models.Customer;
using CarDealsJapanCore.Services;
using CarDealsJapanWeb.Models;
using CarDealsJapanCore.Models.Product;

using System;
using System.Collections.Generic;
using System.Data;
using System.Linq;
using System.Web;
using System.Web.Mvc;

namespace CarDealsJapanWeb.Controllers
{
    public class CommonController : BaseController
    {
        //[OutputCache(VaryByParam = "*", Duration = 0)]
        [ChildActionOnly]
        public ActionResult HeaderAccount()
        {
            //OutputCacheAttribute.IsChildActionCacheActive()
            //var urlToRemove = Url.Action("HeaderAccount", "Common");
            //HttpResponse.RemoveOutputCacheItem(urlToRemove);

            CustomerInfo customerInfoModel = new CustomerInfo();
            string cookie = Helper.GetCookie(Constants.CustomerID, true);

            if (cookie == null)
                return View(customerInfoModel);

            Customer customerService = new Customer();
            DataTable objDt = customerService.GetCustomerDetail(Convert.ToInt32(cookie));
            if (objDt != null && objDt.Rows.Count > 0)
            {
                customerInfoModel.CustomerUid = Convert.ToString(objDt.Rows[0]["CustomerUid"]);
                customerInfoModel.UserName = Convert.ToString(objDt.Rows[0]["FullName"]);
                customerInfoModel.Email = Convert.ToString(objDt.Rows[0]["Email"]);
            }

            return PartialView(customerInfoModel);
        }

        public ActionResult Help()
        {
            return View();
        }

        public ActionResult FAQ()
        {
            return View();
        }

        public ActionResult About()
        {
            return View();
        }
        public ActionResult Testimonials()
        {
            return View();
        }

        public ActionResult PrivacyPolicy()
        {
            return View();
        }

        public ActionResult TermsConditions()
        {
            return View();
        }

        public ActionResult ContactUs()
        {
            return View();
        }

        [HttpPost]
        [ValidateInput(false)]
        [ValidateAntiForgeryToken]
        public ActionResult ContactUs(ContactUsModel contactUsModel)
        {
            if (ModelState.IsValid)
            {
                Customer customerService = new Customer();
                string landingURL = Helper.GetCookie(Constants.LandingURL);
                string referrerURL = Helper.GetCookie(Constants.ReferrerURL);

                DataTable objDt = customerService.AddCustomer(contactUsModel.UserName, contactUsModel.Email, null
                    , (int)CustomerType.Individual, landingURL, referrerURL, (int)SignupPage.ContactUs
                    , Request.Browser.IsMobileDevice ? (int)SignupThrough.Mobile : (int)SignupThrough.Web
                    , Request.UserAgent, (int)CustomerStatus.PreSignupDone, string.Empty);

                if (objDt != null && objDt.Rows.Count > 0)
                {
                    if (Convert.ToBoolean(objDt.Rows[0]["IsAlreadyRegistered"]))
                    {
                        ModelState.AddModelError("", Messages.EmailAlreadyExists);
                        return View(contactUsModel);
                    }
                    else
                    {
                        customerService.AddCustomerContact(Convert.ToInt32(objDt.Rows[0]["CustomerId"]), contactUsModel.CellFlag, contactUsModel.CellNo, null);

                        return RedirectToAction("ThankYou", "Account");
                    }
                }
            }
            return View(contactUsModel);
        }

        [HttpGet]
        [ActionName("Cart")]
        public ActionResult Cart()
        {
            return View();
        }


    }
}
