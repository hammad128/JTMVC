        $('.popup').fadeOut();
        $('.overlay').fadeOut();
        $('body').removeClass('scrolHide');
        $('body').removeClass('hiddenBack');
    });

    $(".datepicker").datepicker({
        dateFormat: "D m/d/yy",
    });

