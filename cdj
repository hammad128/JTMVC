};

var startpoint = 0;
$(window).scroll(function () {
    if ($("div").hasClass("dothecounter")) {
        var e = $(".dothecounter").offset().top - $(window).height();
        var t = $(window).scrollTop();
        if (t >= e) {
            if (startpoint == 0) {
                numbercounter();
                startpoint = 1
            }
        }
    }
});


function reCallSlider(className, imgList, getdata) {

    //var numSlides = $(className).data('cycle.opts').slideCount;
    //for (var slide = 0; slide < numSlides; slide++) {
    //    $(className).cycle('remove', slide);
    //}
    $(className).cycle('destroy');
    for (var i = 0; i < imgList.length; i++) {
        $(className).append(imgList[i]);
    }
    $(className).cycle(getdata);

}



