+BundleConfig.cs

using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;
using System.Web.Optimization;

namespace CarDealsJapanWeb
{
    public class BundleConfig
    {
        public static void RegisterBundles(BundleCollection bundles)
        {
            BundleTable.EnableOptimizations = true;
            bundles.UseCdn = true;
            bundles.Add(new StyleBundle("~/Content/css/bundle").Include(
                "~/Content/css/style.css",
                "~/Content/css/style-responsive.css"
                       ));

            bundles.Add(new ScriptBundle("~/Scripts/bundle").Include(
                "~/Scripts/plugins.js",
                "~/Scripts/functionV2.js",
                "~/Scripts/lightGallery.js",
                "~/Scripts/Helper.js",
                "~/Scripts/jquery.lazyload.min.js"
                      ));

            bundles.Add(new ScriptBundle("~/Scripts/UserArea/bundle").Include(
                "~/Scripts/UserArea/xlibV2.js",
                "~/Scripts/UserArea/functionV2.js",
                "~/Scripts/Helper.js"
          ));


        }
    }
}
+RouteConfig.cs
using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;
using System.Web.Mvc;
using System.Web.Routing;

namespace CarDealsJapanWeb
{
    public class RouteConfig
    {
        public static void RegisterRoutes(RouteCollection routes)
        {
            //routes.IgnoreRoute("{resource}.axd/{*pathInfo}");


            routes.MapRoute(
             name: "AccountInfo",
             url: "user/edit-account",
             defaults: new { controller = "Account", action = "AccountInfo", id = UrlParameter.Optional }
            );

            routes.MapRoute(
               name:"ChangePassword",
               url: "user/edit-password",
               defaults: new { controller = "Account", action = "ChangePassword", id = UrlParameter.Optional }
            );

            routes.MapRoute(
               name: "OrderHistory",
               url: "user/order-history",
               defaults: new { controller = "Account", action = "OrderHistory", id = UrlParameter.Optional }
            );

            routes.MapRoute(
             name: "Stock",
             url: "Stock/{filterId}/{filterCategoryId}",
             defaults: new { controller = "Stock", action = "Index", filterId = UrlParameter.Optional, filterCategoryId = UrlParameter.Optional }
            );

            routes.MapRoute(
              name: "Login",
              url: "Login",
              defaults: new { controller = "Account", action = "Login" }
            );

            routes.MapRoute(
             name: "Register",
             url: "Register",
             defaults: new { controller = "Account", action = "Register" }
           );

            routes.MapRoute(
             name: "PasswordRecovery",
             url: "PasswordRecovery",
             defaults: new { controller = "Account", action = "PasswordRecovery" }
           );

            routes.MapRoute(
             name: "PasswordRecoveryConfirm",
             url: "PasswordRecoveryConfirm",
             defaults: new { controller = "Account", action = "PasswordRecoveryConfirm" }
           );

            routes.MapRoute(
             name: "Partner",
             url: "Partner",
             defaults: new { controller = "Account", action = "Partner" }
           );

