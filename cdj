$(function(){
    $('.profileView').on('click', function() {
		$(this).addClass('showDrop');
		$(this).bind( "clickoutside", function(e){
            $(this).removeClass('showDrop');
        });
    });

	$("select.country").val($("select option[value='Select Country']").val());
    

    $('[id*=txtPhone],[id*=txtCell]').keydown(function (e) {
	    if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 110, 190]) !== -1 ||
            (e.keyCode === 65 && (e.ctrlKey === true || e.metaKey === true)) ||
            (e.keyCode >= 35 && e.keyCode <= 40)) {
	        return;
	    }
	    if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
	        e.preventDefault();
	    }
	});


    $(".addCalendar").daterangepicker({
		applyOnMenuSelect: false,
		 datepickerOptions : {
			 numberOfMonths : 2
		 }
	});
    $(".comiseo-daterangepicker").addClass("order-calendar");


	intlInput(".phoneinput");
	
	$('.selected-flag').hover(function(e){
		$(this).closest('.allow-dropdown').find('input').removeAttr("readonly");
	},function(){
		$(this).closest('.allow-dropdown').find('input').attr("readonly","readonly");
	});

});
	
    $('.closepopup,.overlay').on('click', function() {
        debugger;
        $('.popup').fadeOut();
        $('.overlay').fadeOut();
        $('body').removeClass('scrolHide');
    });


function Popuphistory(popname) {
    $('.popup.' + popname).fadeIn();
    $('.overlay').fadeIn();
    $('body').addClass('scrolHide');
}


function intlInput(target){
	$(target).each(function(){
		$(this).intlTelInput({
			initialCountry: "auto",
			geoIpLookup: function(callback) {
				$.get('http://ipinfo.io', function() {}, "jsonp").always(function(resp) {
					var countryCode = (resp && resp.country) ? resp.country : "";
					callback(countryCode);
				});
			},
			
		});
	});
}


function checkStrength(password) {
    var strength = 0

    if (password.length == 0) {
        return '';
    }

    if (password.length < 6) {
        $('#result').removeClass()
        $('#result').addClass('short')
        return 'Too short'
    }
    if (password.length > 7) strength += 1
    // If password contains both lower and uppercase characters, increase strength value.
    if (password.match(/([a-z].*[A-Z])|([A-Z].*[a-z])/)) strength += 1
    // If it has numbers and characters, increase strength value.
    if (password.match(/([a-zA-Z])/) && password.match(/([0-9])/)) strength += 1
    // If it has one special character, increase strength value.
    if (password.match(/([!,%,&,@@,#,$,^,*,?,_,~])/)) strength += 1
    // If it has two special characters, increase strength value.
    if (password.match(/(.*[!,%,&,@@,#,$,^,*,?,_,~].*[!,%,&,@@,#,$,^,*,?,_,~])/)) strength += 1
    // Calculated strength value, we can return messages
    // If value is less than 2
    if (strength < 2) {
        $('#result').removeClass()
        $('#result').addClass('weak')
        return 'Weak'
    } else if (strength == 2) {
        $('#result').removeClass()
        $('#result').addClass('good')
        return 'Good'
    } else {
        $('#result').removeClass()
        $('#result').addClass('strong')
        return 'Strong'
    }
}

// get the country data from the plugin

var countryData = $.fn.intlTelInput.getCountryData(),
  telInput = $(".phoneinput"),
  addressDropdown = $(".phoneinput");

// populate the country dropdown
$.each(countryData, function(i, country) {
  addressDropdown.append($("<li></li>").attr("value", country.name).text(country.name));
});
// set it's initial value
var initialCountry = telInput.intlTelInput("getSelectedCountryData").name;
addressDropdown.val(initialCountry);

// listen to the telephone input for changes
telInput.on("countrychange", function(e, countryData) {
  addressDropdown.val(countryData.name);
});

// listen to the address dropdown for changes
addressDropdown.change(function() {
  telInput.intlTelInput("setCountry", $(this).val(name));
});

