$(function () {
    $("ul li:first").addClass("first");
    $("ul li:last").addClass("last");

    $('[data-targetit]').on('click', function (e) {
        $(this).addClass('active');
        $(this).siblings().removeClass('active');
        var target = $(this).data('targetit');
        $('.' + target).siblings('[class^="box-"]').hide();
        $('.' + target).fadeIn();

        $(".cycle-slideshow").resize();
    });

    //var iniVal = parseInt($('#countShoppingCart').text());
    $('.add-to-cart').on('click', function () {
        var cart = $('.cart');
        var imgtodrag = $('.cycle-slide-active img');
        if (imgtodrag) {
            var imgclone = imgtodrag.clone().offset({
                top: imgtodrag.offset().top,
                left: imgtodrag.offset().left
            }).css({
                'opacity': '0.5',
                'position': 'absolute',
                'height': '150px',
                'width': '150px',
                'z-index': '100'
            }).appendTo($('body')).animate({
                'top': cart.offset().top + 10,
                'left': cart.offset().left + 10,
                'width': 75,
                'height': 75
            }, 1000, 'easeInOutExpo');

            setTimeout(function () {
                cart.effect("shake", {
                    times: 2
                }, 200);
            }, 1500);

            imgclone.animate({
                'width': 0,
                'height': 0
            }, function () {
                $(this).detach()
            });
            //iniVal = iniVal + 1;
            //$('#countShoppingCart').text(iniVal);
        }
    });


    $('.imageGallery').lightSlider({
        gallery: true,
        item: 1,
        loop: false,
        slideMove: 1,
        thumbItem: 4,
        slideMargin: 0,
        enableDrag: false,
        currentPagerPosition: 'left',
        onSliderLoad: function (el) {
            el.lightGallery({
                selector: '.imageGallery .lslide'
            });
        }
    });

    $('[data-targetit]').on('click', this, function () {
        $('.imageGallery').resize();
    });


