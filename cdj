        $(".details-section ul li a.active").removeClass("active");
        $(this).addClass("active");
    });


    $('.cartList li').on('click', function () {
        var cart = $('.droplist cart');
        var imgtodrag = $(this).parent('.product-img').find("img").eq(0);
        if (imgtodrag) {
            var imgclone = imgtodrag.clone()
                .offset({
                    top: imgtodrag.offset().top,
                    left: imgtodrag.offset().left
                })
                .css({
                    'opacity': '0.5',
                    'position': 'absolute',
                    'height': '150px',
                    'width': '150px',
                    'z-index': '100'
                })
                .appendTo($('body'))
                .animate({
                    'top': cart.offset().top + 10,
                    'left': cart.offset().left + 10,
                    'width': 75,
                    'height': 75
                }, 1000, 'easeInOutExpo');

            setTimeout(function () {
                cart.effect("shake", {
                    times: 2
                }, 200);
            }, 1500);

            imgclone.animate({
                'width': 0,
                'height': 0
            }, function () {
                $(this).detach()
            });
        }
    });

    //intlInput(".phoneinput");
    //intlInput(".phoneinput1");

    $('.currency-list-img img').each(function () {
        var $img = jQuery(this);
        var imgID = $img.attr('id');
        var imgClass = $img.attr('class');
        var imgURL = $img.attr('src');

        jQuery.get(imgURL, function (data) {
            // Get the SVG tag, ignore the rest
            var $svg = jQuery(data).find('svg');

            // Add replaced image's ID to the new SVG
            if (typeof imgID !== 'undefined') {
                $svg = $svg.attr('id', imgID);
            }
            // Add replaced image's classes to the new SVG
            if (typeof imgClass !== 'undefined') {
                $svg = $svg.attr('class', imgClass + ' replaced-svg');
            }

            // Remove any invalid XML tags as per http://validator.w3.org
            $svg = $svg.removeAttr('xmlns:a');

            // Replace image with new SVG
            $img.replaceWith($svg);

        }, 'xml');

    });

});

function imageClick(url) {
    window.location = url;
}


function intlInput(target) {
    $(target).each(function () {
        $(this).intlTelInput({
            initialCountry: "auto",
            geoIpLookup: function (callback) {
                $.get('http://ipinfo.io', function () { }, "jsonp").always(function (resp) {
                    var countryCode = (resp && resp.country) ? resp.country : "";
                    callback(countryCode);
                });
            },
            utilsScript: "assets/js/utils.js",
        });
    });
}

function callPopup(popname) {
    $('.popup.' + popname).fadeIn();
    $('.overlay').fadeIn();
    $('body').addClass('scrolHide');
}

function autocomp(target, data) {
    if ($(target).length !== 0) {
        $(target).each(function () {
            var target = $(this);
            target.autocomplete({
                open: function (event, ui) {
                    //getcontainer(target).parents('.Brandshop').addClass('showUIcomp');

                },
                close: function (event, ui) {
                    //getcontainer(target).parents('.Brandshop').removeClass('active');
                    //console.log(getcontainer(target));
                },
                select: function (event, ui) {
                    if ($(event.target).hasClass('currencyCountry')) {
                        var path = '/content/images/drop-down-img/' + ui.item.icon;
                        $('.currency-list-img').html('');
                        $('.currency-list-img').load(path);
                    } else {
                        console.log('not Selected');
                    }
                },
                source: data,
                minLength: 0,
                appendTo: getcontainer(target),
                response: function (event, ui) {
                    if (ui.content.length === 0) {
                        $(".noResult").show().text("No results found...");
                    } else {
                        $(".noResult").hide();
                    }
                }
            }).focus(function (event, ui) {
                //target.autocomplete('search', $(this).val())
                $(this).autocomplete("search");
            }).data("ui-autocomplete")._renderItem = function (ul, item) {
                return $("<li class='dtbl'>").data("ui-autocomplete-item", item).append("<span class='dtd'><a href='/stock/" + item.id + "/2'><img style='width:78px ; heigth:54px;' class='searchIcon' src='" + brandImagePath  + item.icon + "'>" + item.label + "</a></span>").appendTo(ul);
            };
        });
    }
}

