+ ServiceController.cs

using CarDealsJapanCore.Common;
using CarDealsJapanCore.Models.Services;
using CarDealsJapanCore.Services;
using CarDealsJapanWeb.Models;
using System;
using System.Collections.Generic;
using System.Data;
using System.Linq;
using System.Web;
using System.Web.Mvc;

namespace CarDealsJapanWeb.Controllers
{
    public class ServicesController : BaseController
    {
        public ActionResult ChassisCheck()
        {
            ChassisCheckModel chassisCheckModel = new ChassisCheckModel();
            ProductAttribute productAttribute = new ProductAttribute();

            chassisCheckModel.MakeList = productAttribute.GetMakeList();

            return View(chassisCheckModel);
        }

        [HttpPost]
        [ValidateInput(false)]
        [ValidateAntiForgeryToken]
        public ActionResult ChassisCheck(ChassisCheckModel chassisCheckModel)
        {
            if (ModelState.IsValid)
            {
                Customer customerService = new Customer();
                string landingURL = Helper.GetCookie(Constants.LandingURL);
                string referrerURL = Helper.GetCookie(Constants.ReferrerURL);

                DataTable objDt = customerService.AddCustomer(chassisCheckModel.UserName, chassisCheckModel.Email, null
                        , (int)CustomerType.Individual, landingURL, referrerURL, (int)SignupPage.ChassisCheck
                        , Request.Browser.IsMobileDevice ? (int)SignupThrough.Mobile : (int)SignupThrough.Web
                        , Request.UserAgent, (int)CustomerStatus.PreSignupDone, string.Empty);

                if (objDt != null && objDt.Rows.Count > 0)
                {
                    customerService.AddCustomerContact(Convert.ToInt32(objDt.Rows[0]["CustomerId"]), chassisCheckModel.CellFlag, chassisCheckModel.CellNo, null);

                    Services services = new Services();
                    services.ChassisCheckRequest(Convert.ToInt32(objDt.Rows[0]["CustomerId"]), chassisCheckModel.FrameNumber, chassisCheckModel.MakeId);
                }
            }
            ProductAttribute productAttribute = new ProductAttribute();

            chassisCheckModel.MakeList = productAttribute.GetMakeList();

            return View(chassisCheckModel);
        }

        public ActionResult CarTracking()
        {
            return View();
        }

        public ActionResult ShippingSchedule()
        {
            return View();
        }


        public ActionResult RequestCar()
        {
            RequestCarModel requestCarModel = new RequestCarModel();
            ProductAttribute productAttribute = new ProductAttribute();

            requestCarModel.YearList = productAttribute.GetYearList();
            requestCarModel.CountryList = productAttribute.GetCountryList();

            return View(requestCarModel);
        }

        [HttpPost]
        [ValidateInput(false)]
        [ValidateAntiForgeryToken]
        public ActionResult RequestCar(RequestCarModel requestCarModel)
        {
            if (ModelState.IsValid)
            {
                Customer customerService = new Customer();
                string landingURL = Helper.GetCookie(Constants.LandingURL);
                string referrerURL = Helper.GetCookie(Constants.ReferrerURL);

                DataTable objDt = customerService.AddCustomer(requestCarModel.UserName, requestCarModel.Email, null
                        , (int)CustomerType.Individual, landingURL, referrerURL, (int)SignupPage.RequestCar
                        , Request.Browser.IsMobileDevice ? (int)SignupThrough.Mobile : (int)SignupThrough.Web
                        , Request.UserAgent, (int)CustomerStatus.SignupDone, string.Empty);

                if (objDt != null && objDt.Rows.Count > 0)
                {
                    Services services = new Services();

                    customerService.AddCustomerContact(Convert.ToInt32(objDt.Rows[0]["CustomerId"]), requestCarModel.CellFlag, requestCarModel.CellNo, null);

                    services.CarRequest(Convert.ToInt32(objDt.Rows[0]["CustomerId"]), requestCarModel.StockNo, requestCarModel.Make, requestCarModel.Model
                        , requestCarModel.Year, requestCarModel.CountryFlag, requestCarModel.Comments);

                    return RedirectToAction("ThankYou", "Account");                
                }
            }

            ProductAttribute productAttribute = new ProductAttribute();

            requestCarModel.YearList = productAttribute.GetYearList();
            requestCarModel.CountryList = productAttribute.GetCountryList();

            return View(requestCarModel);
        }

        public ActionResult Auction()
        {


            return View();
        }
    }
}


