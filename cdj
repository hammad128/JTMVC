        $(this).addClass('search-two');
        $(this).find('input');
        $(this).bind("clickoutside", function (e) {
            $(this).removeClass('search-two');
        });
    });

    $('.map-list li').on('click', function () {
        $(this).css({ "z-index": "1" });
        $(this).siblings().css({ "z-index": "0" });
        $(this).find('.details').css({ "transform": "scale(1)", "opacity": "1", "display": "block" });
        $(this).bind("clickoutside", function (e) {
            $(this).find('.details').css({ "display": "none" });
        });
    });

    $('.list a').on('click', function () {
        $(this).addClass('active');
        $('.grid a').removeClass('active');
        $(this).closest('.gridbox').find('.shopbycategory .gridbody').removeClass('listing');
    });

    $('.overlay').on('click', function () {
        $('.popup').fadeOut();
        $(this).fadeOut();
        $('body').removeClass('scrolHide');
    });

    $('#pull').on("click", function () {
        $('#r-menu').slideToggle("slow");
    });

    $('.faqaccordion li div.innerdata ul li, .allCategories').on('click', function () {
        closePopup();
    });

