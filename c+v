                <div align="center">
                    <button class="btn btn-default btn-sm" id="forward" name="forward" onclick="Cancel()">Cancel</button>
                    <button class="btn btn-primary btn-sm" id="forward" name="forward" onclick="Forward()">Save</button>
                </div>
            </div>

        </div>
    </div>
