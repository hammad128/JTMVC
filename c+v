        $("#myModal").hide();
        $("#roles").hide();
        $("#location").hide();
        $("#clients").hide();
        $("#success-alert").hide();
        $("#checkAllroles").hide();
        $("#checkAlllocs").hide();
        ko.applyBindings(UserVMObj, $('.UserVM')[0]);
    })
