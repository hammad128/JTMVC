                                                    @*<h4 class="lfl" id="location"><input type="checkbox" id="checkAlllocs" class="checkkall" style="margin-bottom:10px; margin-right:12px;">Location</h4>*@


                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
