@model CarDealsJapanCore.Models.Customer.AccountInformationModel
@{
    ViewBag.Title = "AccountInformation";
    Layout = "~/Views/Shared/_LayoutAccount.cshtml";
}

@Html.Action("UserNavigation", new { selectedMenu = "account" })

<div class="box-account" style="display:block;">
    <div class="container">
        <div class="account-info">
            <h4><span>Account </span>Information</h4>
            <div class="dtbl">

                @using (Html.BeginForm("AccountInfo", "Account"))
                {
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true, "", new { @class = "text-red" })

                    <div class="dtd wd-px44">
                        <p>On Personal detail</p>
                        <table>
                            <tr>
                                <td>
                                    <div class="pass-wrap">
                                        @Html.TextBoxFor(m => m.Name, new { id = "txtName", placeholder = "Name", autofocus = "autofocus" })
                                        @Html.ValidationMessageFor(m => m.Name, "", new { @class = "field-validation-error error-msg" })
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="pass-wrap">
                                        @Html.TextBoxFor(m => m.Email, new { id = "txtEmail", placeholder = "Email" })
                                         @Html.ValidationMessageFor(m => m.Email, "", new { @class = "text-red error-msg" })
                                    </div>
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div class="dtd">
                        <p>On Contact Detail:</p>
                        <table>
                            <tr>
                                <td>
                                    @Html.HiddenFor(m => m.PhoneFlag, new { id = "hfPhoneFlag" })
                                    @Html.TextBoxFor(m => m.PhoneNo, new { id = "txtPhone" })
                                    @Html.ValidationMessageFor(m => m.PhoneNo, "", new { @class = "text-red" })
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    @Html.HiddenFor(m => m.CellFlag, new { id = "hfCellFlag" })
                                    @Html.TextBoxFor(m => m.CellNo, new { id = "txtCell" })
                                    @Html.ValidationMessageFor(m => m.CellNo, "", new { @class = "text-red" })
                                </td>
                            </tr>
                            <tr>
                                <td><input id="btnAccountInfo" type="submit" class="btnsub" value="Update"></td>
                            </tr>
                        </table>
                    </div>

                    if (string.IsNullOrEmpty(Model.Message))
                    {
                        <div class="green-text">
                            @Model.Message
                        </div>
                    }

                }
            </div>
        </div>
    </div>
</div>



@section Scripts {

    <style>
        .intl-tel-input {
            margin-bottom: 15px;
        }
    </style>

    <script>
        $(document).ready(function () {

            $('[id*=txtPhone],[id*=txtCell]').intlTelInput({
                initialCountry: "auto",
                utilsScript: "/Scripts/utils.js"
            });

            $('[id*=txtPhone]').on("countrychange", function (e, countryData) {
                var countryData = $('[id*=txtPhone]').intlTelInput("getSelectedCountryData");
                $('[id*=hfPhoneFlag]').val(countryData.iso2);
            });

            $('[id*=txtCell]').on("countrychange", function (e, countryData) {
                var countryData = $('[id*=txtCell]').intlTelInput("getSelectedCountryData");
                $('[id*=hfCellFlag]').val(countryData.iso2);
            });

            $('[id*=btnAccountInfo]').click(function () {
                return ValidateForm();
            });

            function ValidateForm() {

                var iserror = false;
                var name = $('[id*=txtName]');
                var email = $('[id*=txtEmail]');
                var phone = $('[id*=txtPhone]');
                var cell = $('[id*=txtCell]');


                name.removeClass('error');
                email.removeClass('error');
                phone.removeClass('error');
                cell.removeClass('error');

                if (name.val().trim() == '' || typeof name.val() == "undefined") {
                    name.addClass('error');
                    iserror = true;
                }
                if (email.val().trim() == '' || typeof email.val() == "undefined" || !Helper.Methods.ValidateEmail(email.val().trim())) {
                    email.addClass('error');
                    iserror = true;
                }
                if (cell.val().trim() == '' || typeof cell.val() == "undefined") {
                    cell.addClass('error');
                    iserror = true;
                }

                return !iserror;
            }

            //Set default country for cell if cell not provided
            var cellFlag = '@Model.CellFlag'  //$('[id*=hfCellFlag]').val();
            var phoneFlag = '@Model.PhoneFlag' //$('[id*=hfPhoneFlag]').val();

            var country = Helper.Methods.GetCookie("country");
            $('[id*=txtCell]').intlTelInput("setCountry", cellFlag != "" ? cellFlag : country);
            $('[id*=txtPhone]').intlTelInput("setCountry", phoneFlag != "" ? phoneFlag : country);

        });
    </script>

}



