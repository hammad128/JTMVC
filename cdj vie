+ _BestSeller.cshtml

<h4>Best Sellers</h4>
<ul class="best-sellers"></ul>



<script type="text/javascript">

    $(document).ready(function () {

        $('#headerCountry').on("countrychange", function (e, countryData) {
            GetBestSellersCountryWise();
        });


        function GetBestSellersCountryWise() {

            var filters = {};
            var searchcountry = Helper.Methods.GetCookie("countrysearch");
            filters.CountryFlag = searchcountry != null ? searchcountry : "";

            Helper.AjaxCall.GetCall('Product/GetFeaturedProducts', filters, function (data) {

                var object = JSON.parse(data);
                if (object.IsSuccess) {

                    var productTemplateHTML = $('#BestSeller').html();
                    $(".best-sellers").html('');

                    var iterations = object.ProductList.length < 5 ? object.ProductList.length : 3;

                    for (var i = 0; i < iterations ; i++) {

                        $(".best-sellers").append(productTemplateHTML.replace("{{Make}}", object.ProductList[i].Make).
                                                                        replace("%7B%7BStockId%7D%7D", object.ProductList[i].StockId).
                                                                        replace("{{Model}}", object.ProductList[i].Model).
                                                                        replace("{{StockID}}", object.ProductList[i].StockId).
                                                                        replace("{{OldPrice}}", formatter.format(object.ProductList[i].Price)).
                                                                        replace("{{ActualPrice}}", object.ProductList[i].Price).
                                                                        replace("{{Grade}}", object.ProductList[i].GradeName).
                                                                        replace("{{JapaneseYen}}", japaneseFormat.format(object.ProductList[i].JapaneseYen)).
                                                                        replace("{{ProductionYear}}", object.ProductList[i].ProductionYear).
                                                                        replace("{{ThumbnailImage}}", productImagePath + object.ProductList[i].ProductImage));

                        $('.rating i').eq(i).css('width', object.ProductList[i].Rating);
                    }
                }
                else {
                    $(".best-sellers").html('');
                }
            });
        }

        GetBestSellersCountryWise();
    });

</script>

<script id="BestSeller" type="text/html">

<li>
    <a href="@Url.Action("ProductDetails", "Product", new { Id = "{{StockId}}" })">
        <div class="floatit"> <span class="img-block">  <img src="{{ThumbnailImage}}" alt="" style="width: 100px; height: 100px;"> </span> </div>
        <div class="best-sellers-text">
            <h3>{{Make}} {{Model}} - {{ProductionYear}}</h3>
            <div class="row">Stock No: {{StockID}} </div>
            @*<div class="row"> <span class="rating"><i style="width: 95% ;position: absolute;font-style: normal;left: 0;top: 0;height: 13px;"></i></span><span class="rating-count">({{Grade}})</span> </div>*@
            <div class="row">
                <span class="price actual-price">{{JapaneseYen}}</span>
            </div>
            <div class="row">
                <span class="price actual-price">{{OldPrice}}</span>
            </div>
        </div>
    </a>
   
</li>
</script>


