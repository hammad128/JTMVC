<script type="text/javascript">


    $(document).ready(function () {

        var cartListObject = [];
        var cartRequestModel = {};



        $(".cartList").on("click", ".delete", function () {

            var cartCookieToRemove = $(this).closest("li").find('input:hidden.hiddenStockIdorDealId').val();
            Helper.Cart.Remove(cartCookieToRemove);

            var stockIdOrDealIdToRemove = $(this).closest("li").find('span.stockid').text();
            $.each(cartListObject, function (index, element) {

                if (element.StockId == stockIdOrDealIdToRemove) {
                    cartListObject.splice(index, 1);
                    return false;
                }
            });
            ShowCartData(cartListObject);
            return false;
        });


        ShowCartData = function (object) {

            if (object.IsSuccess == false  || object.length <= 0) {

                ShowEmptyCart();
            }

            $('.cartList ul').html('');
            var cartTemplate = $('#viewCartTemplate').html();
            var total = 0, count = 0;

            $.each(object, function (index, element) {

                count++;
                total = total + element.Price;

                $('.cartList ul').append(cartTemplate.replace("{{Make}}", element.Make).
                                         replace("{{Model}}", element.Model).
                                         replace("{{StockId}}", element.StockId).
                                         replace("{{ProductionYear}}", element.ProductionYear).
                                         replace("{{Rating}}", element.Rating).
                                         replace("{{Price}}", formatter.format(element.Price)));

                if (element.CartTypeId == 1) {

                    ReplaceCartAccordingToCartType(element, productImagePath, '@Url.Action("ProductDetails", "Product")/', 'P-');
                }

                else {

                    ReplaceCartAccordingToCartType(element, dealsImagePath, '@Url.Action("Index", "Deal")/', 'D-');
                    $('.cartList ul li').last().find('span.instock').css("display", "none");

                }
            });

            $('.itemCount').text(count);
            $('.totalCount').text(formatter.format(total));
        };

        ReplaceCartAccordingToCartType = function (element, imageLinkPath, detailLink, cartCookie) {

            var lastLi = $('.cartList ul li').last().html();
            lastLi = lastLi.replace("{{Image}}", imageLinkPath + element.ProductImage)
                                   .replace("{{DetailLink}}", detailLink + element.StockId)
                                   .replace("{{StockIdorDealId}}", cartCookie + element.StockId);

            $('.cartList ul li').last().html(lastLi);

        };

        ShowEmptyCart = function () {

            $('.leftInner , .rightInner').html('');
            $('.empty-cart').show();
            return false;

        };

        cartRequestModel.CartCookies = Helper.Methods.GetCookie('stockid');
        if (cartRequestModel.CartCookies != null && cartRequestModel.CartCookies != '') {
            Helper.AjaxCall.PostCall('@Url.Action("Cart", "Product")', cartRequestModel, function (data) {

                var object = JSON.parse(data);
                cartListObject = object.ProductList;
                ShowCartData(cartListObject);

            });
        }

        else {

            ShowEmptyCart();
        }


    });
</script>

