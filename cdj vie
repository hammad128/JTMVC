<script type="text/javascript">

    $(document).ready(function () {

        $('body').addClass("home");
        var filters = {};

        $('#headerCountry').on("countrychange", function (e, countryData) {
            GetProductsCountryWise();
        });

        function GetProductsCountryWise() {

            var searchcountry = Helper.Methods.GetCookie("countrysearch");
            filters.CountryFlag = searchcountry != null ? searchcountry : "";

            Helper.AjaxCall.GetCall('Product/GetFeaturedProducts', filters, function (data) {

                var object = JSON.parse(data);
                if (object.IsSuccess) {
                    var productTemplateHTML = $('#featuredproduct').html();
                    $(".feature-products").html('');
                    for (var i = 0; i < object.ProductList.length; i++) {


                        $(".feature-products").append(productTemplateHTML.replace("{{Make}}", object.ProductList[i].Make).
                                                                        replace("%7B%7BStockId%7D%7D", object.ProductList[i].StockId).
                                                                        replace("{{Model}}", object.ProductList[i].Model).
                                                                        replace("{{StockID}}", object.ProductList[i].StockId).
                                                                        replace("{{Price}}", formatter.format(object.ProductList[i].Price)).
                                                                        replace("{{DoorName}}", object.ProductList[i].Door).
                                                                        replace("{{rating}}", object.ProductList[i].Rating).
                                                                        replace("{{Grade}}", object.ProductList[i].GradeName).
                                                                        replace("{{ProductionYear}}", object.ProductList[i].ProductionYear).
                                                                        replace("{{JapaneseYen}}", japaneseFormat.format(object.ProductList[i].JapaneseYen)).
                                                                        replace("{{ProductImage}}", productImagePath + object.ProductList[i].ProductImage));

                        $('.rating i').eq(i).css('width', object.ProductList[i].Rating);
                    }
                }
                else {
                    $(".feature-products").html('');
                }
            });

            Helper.AjaxCall.GetCall('Product/GetFeaturedProducts', filters, function (data) {

                var object = JSON.parse(data);
                if (object.IsSuccess) {

                    var productTemplateHTML = $('#HotDealsProducts').html();
                    $(".HotDeals ul").html('');

                    var iterations = object.ProductList.length < 4 ? object.ProductList.length : 3;
                    for (var i = 0; i < iterations; i++) {

                        $(".HotDeals ul").append(productTemplateHTML.replace("{{Make}}", object.ProductList[i].Make).
                                                                        replace("%7B%7BStockId%7D%7D", object.ProductList[i].StockId).
                                                                        replace("{{Model}}", object.ProductList[i].Model).
                                                                        replace("{{StockId}}", object.ProductList[i].StockId).
                                                                        replace("%7B%7BStockId%7D%7D", object.ProductList[i].StockId).
                                                                        replace("{{ActualPrice}}", formatter.format(object.ProductList[i].Price)).
                                                                        replace("{{ProductionYear}}", object.ProductList[i].ProductionYear).
                                                                        replace("{{JapaneseYen}}", japaneseFormat.format(object.ProductList[i].JapaneseYen)).
                                                                        replace("{{ProductImage}}", productImagePath + object.ProductList[i].ProductImage));

                        $('.rating i').eq(i).css('width', object.ProductList[i].Rating);
                    }
                }
                else {
                    $(".HotDeals ul").html('');
                }
            });
        }

        Helper.AjaxCall.GetCall('Deal/GetDeals', filters, function (data) {
            var object = JSON.parse(data);
            if (object.IsSuccess) {

                var dealTemplate = $('#DealTemplate').html();
                $.each(object.DealList, function (index, element) {
                    $('.offer-slider .cycle-slideshow').cycle('add', dealTemplate.replace("{{DealName}}", element.Name).replace("{{DealImagePath}}", dealsImagePath + element.ThumbnailImage).replace("{{DealId}}", element.DealId));
                });
            }
        });

        GetProductsCountryWise();
    });
</script>

<script id="DealTemplate" type="text/html">
    <img data-cycle-caption1="{{DealName}}" onclick="imageClick('@Url.Action("Index","Deal")/{{DealId}}')" src="{{DealImagePath}}" width="870" height="221" alt="*">
</script>



