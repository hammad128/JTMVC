@section scripts {


    <style type="text/css">

        .disabled {
            opacity: 0.4;
            pointer-events: none;
        }
    </style>


    <script type="text/javascript">


        $(document).ready(function () {


            var CurrentPage = 1;
            var totalPage;
            var pageSize = 10;

            //var todaysDate = new Date();
            //todaysDate.setDate(todaysDate.getDate() + (-10));
            //$('#fromDate').datepicker({ dateFormat: 'dd-mm-yy' }).datepicker("setDate", new Date());
            //$("#toDate").datepicker({ dateFormat: 'dd-mm-yy' }).datepicker("setDate", new Date());


            $(".datepicker").datepicker({
                dateFormat: "D m/d/yy",
            });

            $(".search .shadow").click(function () {
                GetOrdersHistory(1);
            });


            $(".pagination ul").on("click", ".x-right-arrow", function () {
                GetOrdersHistory(CurrentPage + 1);
            });

            $(".pagination ul").on("click", ".x-left-arrow", function () {
                GetOrdersHistory(CurrentPage - 1);
            });

            GetOrdersHistory = function(currentPage) {

                var data = {};
                debugger;
                
                var from = $('#fromDate').datepicker("getDate");
                var to  = $('#toDate').datepicker("getDate");
                
                data.FromDate = from == null ?  from  :   from.toISOString();
                data.ToDate = to == null ?  to : to.toISOString();
                data.PageSize = pageSize;
                data.PageNumber = currentPage;
                //To preserve value for next and previous button
                CurrentPage = currentPage;


                Helper.AjaxCall.PostCall('@Url.Action("GetOrderHistory", "Account")', data, function (data) {
                    var object = data;

                    if (object.IsSuccess) {

                        $(".detail-table .detail-table-box").html('');
                        var from = (currentPage - 1) * pageSize + 1;
                        var to = (from + object.OrderHistoryList.length - 1);

                        $.each(object.OrderHistoryList, function (index, element) {

                            var template = $("#orderHistoryTemplate").html();
                            $(".detail-table .detail-table-box").append(template.replace("{{SNo}}", from + index)
                                                                        .replace("{{OrderNo}}", element.OrderNo)
                                                                        .replace("{{OrderId}}", element.OrderId)
                                                                        .replace("{{OrderDate}}", element.Date)
                                                                        .replace("{{Status}}", element.Status)
                                                                        .replace("{{Amount}}", formatter.format(element.Amount))
                                                                        .replace("{{Quantity}}", element.Quantity));
                        });


                        $(".pagination ul").html('');
                        var paginationTemplate = $('#paginationTemplate').html();

                        $(".totalrecords").text(object.TotalRecords);
                        $(".resultsrange").text(from + " - " + to);

                        $(".pagination ul").append('<li class=""><a href="javascript:;"><i class="x-left-arrow  ' + (currentPage == 1 ? "disabled" : "") + '"></i></a></li>');
                        totalPage = Math.ceil(object.TotalRecords / pageSize);

                        var endIndex = totalPage;
                        var startIndex = 1;

                        if (totalPage >= 5) {
                            startIndex = currentPage - 1;
                            endIndex = currentPage + 3;

                            if (startIndex <= 0) {
                                startIndex = 1;
                                endIndex = startIndex + 4;
                            }

                            if (endIndex > totalPage) {
                                endIndex = totalPage;
                                startIndex = totalPage - 4;
                            }
                        }

                        for (var i = startIndex ; i <= endIndex ; i++) {

                            $(".pagination ul").append(paginationTemplate.replace("{{i}}", i)
                                                                         .replace("{{i}}", i)
                                                                         .replace("{{anchorClassName}}", i == CurrentPage ?  "active" : ""));
                        }

                        $(".pagination ul").append('<li><a class="" href="javascript:;"><i class="x-right-arrow ' + (currentPage == endIndex ? "disabled" : "") +  '"></i></a></li>');
                    }

                    else {

                        $(".detail-table .detail-table-box").html('No Records Found');
                        $(".detail-table .detail-table-box").addClass('no-record');
                        $(".pagination ul").html('');
                        $(".totalrecords").text(0);
                        $(".resultsrange").text(0);

                    }
                });

            }

            GetOrdersDetailHistory = function (orderId, reference) {

                var orderNo = $(reference).closest('li').siblings('li').find('.OrderNo').text();
                var amount = $(reference).closest('li').siblings('li').find('.OrderAmount').text();

                Helper.AjaxCall.GetCall('@Url.Action("GetOrderHistoryDetail", "Account")/' + orderId, {}, function (data) {

                    var object = data;
                    if (object.IsSuccess) {

                        Popuphistory('res-detail');

                        var $productReference = $(".popup.res-detail .his-pop-wrap .items-wrap ul")
                        $productReference.html('');

                        $.each(object.ProductList, function (index, element) {

                            var template = $("#OrderDetailHistoryTemplate").html();
                            $productReference.append(template.replace("{{ProductImage}}", productImagePath + element.ProductImage)
                                                                        .replace("{{Make}}", element.Make)
                                                                        .replace("%7B%7BStockId%7D%7D", element.StockId)
                                                                        .replace("{{Model}}", element.Model)
                                                                        .replace("{{ProductionYear}}", element.ProductionYear)
                                                                        .replace("{{Price}}", formatter.format(element.Price))
                                                                        .replace("{{StockId}}", element.StockId));
                        });

                        $('.his-pop-wrap h4 span').text(orderNo);
                        $('.total-rate').html('<p><span>Sub-Total </span>' + amount + ' </p>')
                        $('.his-pop-wrap li.wd-px47 span').text('Items ' + object.ProductList.length);

                    }

                    else {

                        $productReference.html('No Records Found');
                    }
                });

            }

            $('.close').on('click', function () {
                $('.popup').fadeOut();
                $('.overlay').fadeOut();
                $('body').removeClass('scrolHide');
            });

            GetOrdersHistory(1);
        });


    </script>


    <script id="orderHistoryTemplate" type="text/html">
        <ul class="dtbl border-detail">
            <li class="dtd wd-px5">
                <p class="det">{{SNo}}</p>
            </li>
            <li class="dtd wd-px18">
                <p class="det OrderNo">{{OrderNo}}</p>
            </li>
            <li class="dtd wd-px20">
                <p class="det">{{OrderDate}}</p>
            </li>
            <li class="dtd wd-px29">
                <p class="det">{{Status}}</p>
            </li>
            <li class="dtd wd-px18">
                <p class="det OrderAmount">{{Amount}}</p>
            </li>
            <li class="dtd wd-px17">
                <p class="det">{{Quantity}}</p>
            </li>
            <li class="dtd wd-px10">
                <p class="det1" onclick="GetOrdersDetailHistory('{{OrderId}}' , this)">View Detail</p>
            </li>
        </ul>
    </script>

    <script id="OrderDetailHistoryTemplate" type="text/html">
        <li>
            <ul class="dtbl">
                <li class="dtd wd-px165">
                    <img src="{{ProductImage}}" style="width:130px;height:100px;" alt="...">
                </li>
                <li class="dtd wd-px305">

                    <a href="@Url.Action("ProductDetails", "Product", new { Id = "{{StockId}}" })">
                        <h3>{{Make}} {{Model}} <br /> {{ProductionYear}}</h3>
                    </a>
                    @*<i class="rating"></i>*@
                    <p>StockNo: {{StockId}}</p>
                </li>
                <li class="dtd">
                    <p class="price">{{Price}}</p>
                </li>
            </ul>
        </li>
    </script>

    <script id="paginationTemplate" type="text/html">
        <li><a class="det pageno {{anchorClassName}}" href="javascript:;" onclick="GetOrdersHistory({{i}})">{{i}}</a></li>
    </script>


}

+ Partner.cshtml

@model CarDealsJapanCore.Models.Customer.PartnerModel
@{
    ViewBag.Title = "Partner";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style type="text/css">
    .formreq .short {
        font-weight: bold;
        color: #FF0000;
        font-size: larger;
    }

    .formreq .weak {
        font-weight: bold;
        color: orange;
        font-size: larger;
    }

    .formreq .good {
        font-weight: bold;
        color: #2D98F3;
        font-size: larger;
    }

    .formreq .strong {
        font-weight: bold;
        color: limegreen;
        font-size: larger;
    }
</style>

<div class="center-3">
    <section class="affiliate">
        <article>
            <ul class="breadcrumbs">
                <li><a href="index.asp">Home</a></li>
                <li><a href="#">Become An Affiliate Program </a></li>
            </ul>
            <div class="banner-inner banner-inner-affiliate">
                <h3>Become An Affiliate Program </h3>
            </div>
            <div class="req-head">
                <h4>Registration <span>Information</span></h4>
            </div>
            <div class="formreq">
                @using (Html.BeginForm("Partner", "Account"))
                {
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true, "", new { id = "lblValidationSummary", @class = "text-red" })

                    <table>
                        <tr>
                            <td width="170"><label>Full Name <span>*</span></label></td>
                            <td>
                                @Html.TextBoxFor(m => m.UserName, new { id = "txtName", placeholder = "Enter your name", autofocus = "autofocus" })
                                @Html.ValidationMessageFor(m => m.UserName, "", new { @class = "text-red" })
                            </td>
                        </tr>
                        <tr>
                            <td width="170"><label>Country </label></td>
                            <td>
                                @Html.DropDownListFor(m => m.CountryFlag, Model.CountryList, new { id = "ddlCountry" })
                            </td>
                        </tr>
                        <tr>
                            <td width="170"><label>Port</label></td>
                            <td>
                                <select id="ddlPort"></select>
                            </td>
                        </tr>
                        <tr>
                            <td width="170"><label>Email <span>*</span></label></td>
                            <td>
                                @Html.TextBoxFor(m => m.Email, new { id = "txtEmail", placeholder = "Enter your email" })
                                @Html.ValidationMessageFor(m => m.Email, "", new { @class = "text-red" })
                            </td>
                        </tr>
                        <tr>
                            <td width="170"><label>Phone no <span>*</span></label></td>
                            <td>
                                @Html.HiddenFor(m => m.CellFlag, new { id = "hfCellFlag" })
                                @Html.TextBoxFor(m => m.CellNo, new { id = "txtPhone" })
                                @Html.ValidationMessageFor(m => m.CellNo, "", new { @class = "text-red" })                                
                            </td>
                        </tr>
                        <tr>
                            <td width="170"><label>Password <span>*</span></label></td>
                            <td>
                                @Html.PasswordFor(m => m.Password, new { id = "txtPassword", placeholder = "Enter your password" })
                                <span id="result"></span>
                                @Html.ValidationMessageFor(m => m.Password, "", new { @class = "text-red" })
                            </td>
                        </tr>
                        <tr>
                            <td width="170"><label>Confirm Password <span>*</span></label></td>
                            <td>
                                @Html.PasswordFor(m => m.ConfirmPassword, new { id = "txtConfirmPassword", placeholder = "Enter confirm password" })
                                @Html.ValidationMessageFor(m => m.ConfirmPassword, "", new { @class = "text-red" })
                            </td>
                        </tr>
                    </table>
                    <div class="req-head"><h4>Select <span>Package</span></h4></div>
                    <table>
                        <tr>
                            <td><label>Dealer Type</label></td>
                            <td>
                                @Html.DropDownListFor(m => m.CustomerType, Model.CustomerTypeList)
                            </td>
                        </tr>
                        <tr>
                            <td width="170"></td>
                            <td><input id="btnRegister" type="submit" name="sub" class="btnsub" value="Submit"></td>
                        </tr>
                    </table>
                }
            </div>
        </article>
    </section>
</div>


<style>

    .intl-tel-input {
        margin-bottom: 20px;
    }

</style>

<script type="text/javascript">
    $(document).ready(function () {

        $('[id*=txtPhone]').intlTelInput({
            initialCountry: "auto",
            utilsScript: "/Scripts/utils.js"
        });

        $('[id*=txtPassword]').keyup(function () {
            $('#result').html(checkStrength($('[id*=txtPassword]').val()));
        });

        $('[id*=txtPhone]').on("countrychange", function (e, countryData) {
            var countryData = $('[id*=txtPhone]').intlTelInput("getSelectedCountryData");            
            $('[id*=hfCellFlag]').val(countryData.iso2);
        });

        $('[id*=txtPhone]').keydown(function (e) {
            if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 110, 190]) !== -1 ||
                (e.keyCode === 65 && (e.ctrlKey === true || e.metaKey === true)) ||
                (e.keyCode >= 35 && e.keyCode <= 40)) {
                return;
            }
            if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
                e.preventDefault();
            }
        });

        $('[id*=btnRegister]').click(function () {
            return validateForm();
        });

        $('[id*=ddlCountry]').change(function () {
            getPorts();
        });

        var country = Helper.Methods.GetCookie("country");
        if (country != null) {
            $('[id*=txtPhone]').intlTelInput("setCountry", country);
            $('[id*=ddlCountry]').val(country);
        }
    });

    function validateForm() {
        var iserror = false;
        var name = $('[id*=txtName]');
        var email = $('[id*=txtEmail]');
        var phone = $('[id*=txtPhone]');
        var password = $('[id*=txtPassword]');
        var confirmPassword = $('[id*=txtConfirmPassword]');        

        name.removeClass('error');
        email.removeClass('error');
        phone.removeClass('error');
        password.removeClass('error');
        confirmPassword.removeClass('error');

        if (name.val().trim() == '' || typeof name.val() == "undefined") {
            name.addClass('error');
            iserror = true;
        }
        if (email.val().trim() == '' || typeof email.val() == "undefined" || !Helper.Methods.ValidateEmail(email.val().trim())) {
            email.addClass('error');
            iserror = true;
        }
        if (phone.val().trim() == '' || typeof phone.val() == "undefined") {
            phone.addClass('error');
            iserror = true;
        }
        if (password.val().trim() == '' || typeof password.val() == "undefined") {
            password.addClass('error');
            iserror = true;
        }
        if (confirmPassword.val().trim() == '' || typeof confirmPassword.val() == "undefined") {
            confirmPassword.addClass('error');
            iserror = true;
        }
        //if (!iserror && password.val().length < 6) {
        //    $('[id*=lblValidationSummary]').html("Password is too short.");
        //    iserror = true;
        //}
        //else if (!iserror && password.val().length > 20) {
        //    $('[id*=lblValidationSummary]').html("Password is too long.");
        //    iserror = true;
        //}
        //else if (!iserror && password.val().trim() != confirmPassword.val().trim()) {
        //    $('[id*=lblValidationSummary]').html("Password doesn't match.");
        //    iserror = true;
        //}       

        return !iserror;
    }

    function getPorts() {
        $('[id*=ddlPort]').empty();
        Helper.AjaxCall.GetCall('ProductAttribute/GetPorts', { id: $('[id*=ddlCountry]').val() }, function (data) {
            var ports = JSON.parse(data);
            $.each(ports, function (i, port) {
                $('[id*=ddlPort]').append('<option value="' + port.PortId + '">' +
                     port.PortName + '</option>');
            });
        });
    }

    function checkStrength(password) {
        var strength = 0

        if (password.length == 0){
            return '';
        }

        if (password.length < 6) {
            $('#result').removeClass()
            $('#result').addClass('short')
            return 'Too short'
        }
        if (password.length > 7) strength += 1
        // If password contains both lower and uppercase characters, increase strength value.
        if (password.match(/([a-z].*[A-Z])|([A-Z].*[a-z])/)) strength += 1
        // If it has numbers and characters, increase strength value.
        if (password.match(/([a-zA-Z])/) && password.match(/([0-9])/)) strength += 1
        // If it has one special character, increase strength value.
        if (password.match(/([!,%,&,@@,#,$,^,*,?,_,~])/)) strength += 1
        // If it has two special characters, increase strength value.
        if (password.match(/(.*[!,%,&,@@,#,$,^,*,?,_,~].*[!,%,&,@@,#,$,^,*,?,_,~])/)) strength += 1
        // Calculated strength value, we can return messages
        // If value is less than 2
        if (strength < 2) {
            $('#result').removeClass()
            $('#result').addClass('weak')
            return 'Weak'
        } else if (strength == 2) {
            $('#result').removeClass()
            $('#result').addClass('good')
            return 'Good'
        } else {
            $('#result').removeClass()
            $('#result').addClass('strong')
            return 'Strong'
        }
    }

</script>

