<script type="text/javascript">
    $(document).ready(function () {

        $(".info-ul").on('click', '.btnsub.this-edit', function () {

            var jsondata = $(this).closest('li').find('.cstAdId').val();
            SetFormData(jsondata);

        });


        $(".info-ul").on('click', '.btnsub.this-add', function () {

            var JSONdata = $(this).closest('li').find('.cstAdId').val();
            var addressId = JSON.parse(JSONdata).AddressId;
            $("#AddressId").val(addressId);
            SetFormData(JSONdata);
            SetCartValues();

            $('#ShippingForm').submit();

        });

        function SetCartValues() {

            var cartCookies = Helper.Methods.GetCookie('stockid');
            $("#CartIds").val(cartCookies);

        }


        function SetFormData(JSONdata) {

            var data = JSON.parse(JSONdata);
            $("#txtAddressLine1").val(data.AddressLine1);
            $("#txtAddressLine2").val(data.AddressLine2);
            $("#txtCity").val(data.City);
            $("#txtZip").val(data.Zip);
            $("#txtPhone").val(data.CellNo);
            $("#txtName").val(data.UserName);
            $('#ddlCountry').val(data.CountryFlag);
            data.CellFlag != "" ? $('[id*=txtPhone]').intlTelInput("setCountry", data.CellFlag) : "";
        }


        $(".info-ul").on('click', '.btnsub.this-del', function () {

            var JSONdata = $(this).closest('li').find('.cstAdId').val();
            var Id = JSON.parse(JSONdata).AddressId;
            var currentDeleteElement = $(this).closest('li');

            Helper.AjaxCall.GetCall('@Url.Action("DeleteShippingAddress", "Account")/' + Id, {}, function (data) {

                if (data.IsSuccess) {
                    currentDeleteElement.remove();
                    $('.message-text').text('@CarDealsJapanCore.Common.Messages.DeleteShippingAddress');
                    $('.message-inner').show().delay(5000).fadeOut();
                    $("html, body").animate({ scrollTop: 0 }, "slow");
                }   

                else {
                    console("Invalid Response !!!");
                }

            }, function (error) { }, RequestDomainEnum.Website);
        });

        $('.close-off').click(function () {
            $('.message-inner').hide().fadeOut();
        });

        $('[id*=txtPhone]').intlTelInput({
            initialCountry: "auto",
            utilsScript: "/Scripts/utils.js"
        });

        $('[id*=txtPhone]').on("countrychange", function (e, countryData) {
            var countryData = $('[id*=txtPhone]').intlTelInput("getSelectedCountryData");
            $('[id*=hfCellFlag]').val(countryData.iso2);
        });

        $('[id*=txtPhone]').keydown(function (e) {
            if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 110, 190]) !== -1 ||
                (e.keyCode === 65 && (e.ctrlKey === true || e.metaKey === true)) ||
                (e.keyCode >= 35 && e.keyCode <= 40)) {
                return;
            }
            if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
                e.preventDefault();
            }
        });

        $('[id*=btnShip]').click(function () {
            SetCartValues();
            return validateForm();
        });

        $('[id*=ddlDestinationCountry]').change(function () {
            getPorts();
        });

        $('[id*=ddlCountry]').change(function () {
            getStates();
        });

        debugger;
        var cellFlag = $('[id*=hfCellFlag]').val();
        var country = Helper.Methods.GetCookie("country");
        $('[id*=ddlCountry]').val(country);
        $('[id*=txtPhone]').intlTelInput("setCountry", cellFlag != "" ? cellFlag : country);


    });

    function validateForm() {
        var iserror = false;
        var name = $('[id*=txtName]');
        var addressLine1 = $('[id*=txtAddressLine1]');
        var phone = $('[id*=txtPhone]');
        var city = $('[id*=txtCity]');
        var zip = $('[id*=txtZip]');

        name.removeClass('error');
        addressLine1.removeClass('error');
        phone.removeClass('error');
        city.removeClass('error');
        zip.removeClass('error');

        if (name.val().trim() == '' || typeof name.val() == "undefined") {
            name.addClass('error');
            iserror = true;
        }
        if (addressLine1.val().trim() == '' || typeof addressLine1.val() == "undefined") {
            addressLine1.addClass('error');
            iserror = true;
        }
        if (phone.val().trim() == '' || typeof phone.val() == "undefined") {
            phone.addClass('error');
            iserror = true;
        }
        if (city.val().trim() == '' || typeof city.val() == "undefined") {
            city.addClass('error');
            iserror = true;
        }
        if (zip.val().trim() == '' || typeof zip.val() == "undefined") {
            zip.addClass('error');
            iserror = true;
        }

        return !iserror;
    }

    function getPorts() {
        $('[id*=ddlPort]').empty();
        Helper.AjaxCall.GetCall('ProductAttribute/GetPorts', { id: $('[id*=ddlDestinationCountry]').val() }, function (data) {
            var ports = JSON.parse(data);
            $.each(ports, function (i, port) {
                $('[id*=ddlPort]').append('<option value="' + port.PortId + '">' +
                     port.PortName + '</option>');
            });
        });
    }

    function getStates() {
        $('[id*=ddlState]').empty();
        Helper.AjaxCall.GetCall('ProductAttribute/GetStates', { id: $('[id*=ddlCountry]').val() }, function (data) {
            var states = JSON.parse(data);
            $.each(states, function (i, state) {
                $('[id*=ddlState]').append('<option value="' + state.StateId + '">' +
                     state.StateName + '</option>');
            });
        });
    }
</script>


