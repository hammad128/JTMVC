<script type="text/javascript">

    $(document).ready(function () {

        var dealId = '@ViewBag.DealId';  

        Helper.AjaxCall.GetCall('Deal/Dealdetails/' + dealId , {}, function (data) {

            var object = JSON.parse(data);

            if (object.IsSuccess) {
                debugger;
                var isExist = Helper.Methods.IsProductExistInCookie('stockid', dealId , CarTypeEnum.Deal);
                if (isExist) {
                    DisableClickOnAdToCart();
                }



                $('.deals-head-sec .deal-head p').html('Contains <strong>' + object.ProductList.length + '</strong> Cars');
                $('.deals-head-sec .deal-head h3').html('<span>' + object.DealDetails.Name + '</span>');
                $('.deals-head-sec .deal-head h4').html('Price: <span>' + formatter.format(object.DealDetails.Price) + '</span>');


                var productTemplateHTML = $('#DealsExcerpt').html();
                $(".details-section ul").html('');

                for (var i = 0; i < object.ProductList.length; i++) {


                    $(".details-section ul").append(productTemplateHTML.replace("{{Make}}", object.ProductList[i].Make).
                                                                    replace("{{StockID}}", object.ProductList[i].StockId).
                                                                    replace("{{Model}}", object.ProductList[i].Model).
                                                                    replace("{{StockID}}", object.ProductList[i].StockId).
                                                                    replace("{{rating}}", object.ProductList[i].Rating).
                                                                    replace("{{Grade}}", object.ProductList[i].GradeName).
                                                                    replace("{{ProductionYear}}", object.ProductList[i].ProductionYear).
                                                                    //replace("{{Price}}", formatter.format(object.ProductList[i].Price)).
                                                                    //replace("{{JapaneseYen}}", japaneseFormat.format(object.ProductList[i].JapaneseYen)).
                                                                    replace("{{ProductImage}}", productImagePath + object.ProductList[i].ProductImage));
                    //$('.rating i').eq(i).css('width', object.ProductList[i].Rating);
                }

                $(".details-section ul li:first a").trigger("click");
                //GetProductDetails(object.ProductList[0].StockId);

            }
            else {
                $(".details-section ul").append('<li>No deal found</li>');
            }
        });

        $('.details-section').on('click', 'ul li a', function () {

            $(".details-section ul li a.active").removeClass("active");
            $(this).addClass("active");
            var stockId = $(this).closest('li').find('input:hidden.hiddenProductId').val();
            GetProductDetails(stockId);

        });

        $('.close').click(function () {
            $('#bar-notification').hide().fadeOut();
        });


        $('.add-to-cart').click(function () {

            var dealID = dealId; 
            Helper.Cart.Add(dealID, CarTypeEnum.Deal);
        });

        var data1 = {
            slides: "> div",
            prev: ".slideshow-1 .cycle-prev",
            next: ".slideshow-1 .cycle-next",
            caption: ".slideshow-1 .custom-caption",
            captionTemplate: "Slide {{slideNum}} of {{slideCount}}",
            fx: "tileBlind",
            speed: 700,
            timeout: 5000,
            log: false,
            allowWrap: false
        }

        var data2 = {
            slides: "> div",
            timeout: 0,
            prev: ".thisone-2 .cycle-prev1",
            next: ".thisone-2 .cycle-next1",
            caption: ".thisone-2 .custom-caption",
            captionTemplate: "Slide {{slideNum}} of {{slideCount}}",
            fx: "carousel",
            carouselVisible: 4,
            carouselFluid: true,
            allowWrap: false
        }
      
        GetProductDetails = function (stockId) {

            Helper.AjaxCall.GetCall('@Url.Action("GetProductDetails", "Product")' + '/' + stockId, {}, function (data) {
                var object = JSON.parse(data);
                if (object.IsSuccess) {


                    if (object.AuctionSheet == 0) {
                        $('.auction-sheet').css("display", "none");                       }

                    else {
                        var auctionSheetPath = commonImagesPath + object.AuctionSheet;
                        $('.auction-sheet .download-auction').attr('href', auctionSheetPath);
                        $('.popupWrap .closepopup .box img').attr('src', auctionSheetPath);
                        $('.auction-sheet').css("display", "block");

                    }

                    $('.rightside #first h3').text(object.Make + '  ' + object.Model);

                    $('#StockId').text(object.StockId);
                    $('#RegistrationYear').text(object.RegistrationYear);
                    $('#Transmission').text(object.Transmission);
                    $('#Drive').text(object.Drive);
                    $('#Steering').text(object.Steering);
                    $('#EngineType').text(object.EngineType);
                    $('#EngineSize').text(object.EngineSize == "N/A" ? "" : object.EngineSize + " CC");
                    $('#LoadingCapacity').text(object.LoadingCapacity);
                    $('#Fuel').text(object.Fuel);
                    $('#Model').text(object.Model);
                    $('#ProductionYear').text(object.ProductionYear);
                    $('#Color').text(object.Color);
                    $('#Door').text(object.Door == "N/A" ? "" : object.Door + " door");
                    $('#Seat').text(object.Seat);
                    $('#BodyType').text(object.BodyType);
                    $('.list-price strong').text(formatter.format(object.Price));


                    $(".product-access ul").html('');
                    $.each(object.ProductFeatureList, function (index, element) {

                        //var imagePath = commonImagesPath + (element.IsFeatureInclude ? 'd-tick-red.png' : 'd-tick-grey.png');
                        //$(".product-access ul").append('<li><img src="' + imagePath + '" alt=""><a href="javascript:;">' + element.FeatureName + '</a></li>');

                        var className = (element.IsFeatureInclude ? 'tick-red' : 'tick-grey');
                        $(".product-access ul").append('<li><a href="javascript:;"><i class="' + className + '"></i>' + element.FeatureName + '</a></li>');


                    });

                    var numSlides = $('.slideshow-1 .cycle-slideshow').data('cycle.opts').slideCount;
                    //var imageslist1 = [] , imageslist2 = [];

                    for (var slide = 0; slide < numSlides; slide++) {
                        $('.slideshow-1 .cycle-slideshow').cycle('remove', 0);
                        $('.slideshow-2 .cycle-slideshow').cycle('remove', 0);
                    }

                    $.each(object.ProductImagesList, function (index, element) {
                        $('.slideshow-1 .cycle-slideshow').cycle('add', '<div><img src="' + productImagePath + element.ImageCode + '" alt="slider' + index + '" /></div>');
                        $('.slideshow-2 .cycle-slideshow').cycle('add', '<div><img src="' + productImagePath + element.ThumbnailCode + '" alt="slider' + index + '" /></div>');

                        //imageslist1.push('<div><img src="' + productImagePath + element.ImageCode + '" alt="slider' + index + '" /></div>');
                        //imageslist2.push('<div><img src="' + productImagePath + element.ThumbnailCode + '" alt="slider' + index + '" /></div>');

                    });

                    //reCallSlider('.slideshow-1 .cycle-slideshow', imageslist1, data1);
                    //reCallSlider('.slideshow-2 .cycle-slideshow', imageslist2, data2);

                }
            });

        }
    });


</script>


