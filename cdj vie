@*No need to add document.ready function for this code other wise it will
    be execute on last and unable to apply css immediately*@
<script>
    $('body').addClass('order-history');
</script>


<div class="tab-content">
    <div class="container">
        <ul>
            <li data-targetit="box-order"><a href='@Url.Action("OrderHistory","Account")'>Order History</a></li>
            <li data-targetit="box-account"><a href='@Url.Action("AccountInfo","Account")'>Account Information</a></li>
            <li data-targetit="box-change"><a href='@Url.Action("ChangePassword","Account")'>Change Password</a></li>
        </ul>
    </div>
</div>

<script>

    $(document).ready(function () {

        var dataTargetName = '@ViewBag.DataTargetValue';
        var elementReference = $('.order-history .tab-content ul li[data-targetit=' + dataTargetName + ']');

        elementReference.addClass('active');
        elementReference.siblings().removeClass('active');
        var target = elementReference.data('targetit');
        $('.' + target).siblings('[class^="box-"]').hide();
        $('.' + target).fadeIn();

    })

</script>


