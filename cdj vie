                @{Html.RenderPartial("_Message");}


                <h4>Select  a <span>Shipping</span> Address</h4>

                <ul class="dtbl info-ul">

                    @foreach (var item in Model.AddressList)
                    {
                        <li class="dtd wd-px385">
