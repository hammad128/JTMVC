<script>

    $(document).ready(function () {

        var filters = {};
       
        var data1 = {
            slides: "> div",
            prev: ".slideshow-1 .cycle-prev",
            next: ".slideshow-1 .cycle-next",
            fx: 'tileBlind',
            speed: 700,
            timeout: 5000,
            log: false,
            allowWrap: false
        }

        var data2 = {
            slides: "> div",
            timeout: 0,
            prev: ".slideshow-2 .cycle-prev",
            next: ".slideshow-2 .cycle-next",
            fx: "carousel",
            carouselFluid: true,
            allowWrap: true,
            log: false
        }

        $('.close').click(function () {
            $('#bar-notification').hide().fadeOut();
        });

        $('.add-to-cart').click(function () {

            var stockId = $('#StockId').text();
            Helper.Cart.Add(stockId, CarTypeEnum.Product);
        });

        Helper.AjaxCall.GetCall('@Url.Action("GetProductDetails", "Product", new { Id = ViewBag.StockId })', {}, function (data) {
            var object = JSON.parse(data);

            if (object.IsSuccess) {

                var isExist = Helper.Methods.IsProductExistInCookie('stockid', object.StockId, CarTypeEnum.Product);
                if (isExist){
                    DisableClickOnAdToCart();
                }


                $('.leftside h2').first().text(object.Make + ' - ' + object.Model + ' ' +  object.ProductionYear)

                $('#StockId').text(object.StockId);
                $('#RegistrationYear').text(object.RegistrationYear);
                $('#Transmission').text(object.Transmission);
                $('#Drive').text(object.Drive);
                $('#Steering').text(object.Steering);
                $('#EngineType').text(object.EngineType);
                $('#EngineSize').text(object.EngineSize == "N/A" ? "" : object.EngineSize + " CC");
                $('#LoadingCapacity').text(object.LoadingCapacity);
                $('#Fuel').text(object.Fuel);
                $('#Model').text(object.Model);
                $('#ProductionYear').text(object.ProductionYear);
                $('#Color').text(object.Color);
                $('#Door').text(object.Door == "N/A" ? "" : object.Door + " door");
                $('#Seat').text(object.Seat);
                $('#BodyType').text(object.BodyType);
                $('.list-price strong').text(formatter.format(object.Price));

                filters.MakeId = object.MakeId;
                CallRelativeProduct();

                if (object.AuctionSheet != 0) {

                    var auctionSheetPath = commonImagesPath + object.AuctionSheet;
                    $('.auction-sheet .download-auction').attr('href', auctionSheetPath);
                    $('.popupWrap .closepopup .box img').attr('src', auctionSheetPath);
                    $('.auction-sheet').css("display", "block");
                }

                $.each(object.ProductFeatureList, function (index, element) {

                    //var imagePath = commonImagesPath + (element.IsFeatureInclude ? 'tick-green.png' : 'tick-grey.png');
                    //$(".product-access ul").append('<li><img src="' + imagePath + '" alt=""><a href="javascript:;">' + element.FeatureName + '</a></li>');

                    var className = (element.IsFeatureInclude ? 'green' : 'grey');
                    $(".product-access ul").append('<li><a href="javascript:;"><i class="' + className + '"></i>' + element.FeatureName + '</a></li>');

                });

                var imageslist1 = [], imageslist2 = [];

                $.each(object.ProductImagesList, function (index, element) {

                    //$('.slideshow-1 .cycle-slideshow').cycle('add', '<div><img src="' + productImagePath + element.ImageCode + '" alt="slider' + index + '" /></div>');
                    //$('.slideshow-2 .cycle-slideshow').cycle('add', '<div><img src="' + productImagePath + element.ThumbnailCode + '" alt="slider' + index + '" /></div>');

                    imageslist1.push('<div><img src="' + productImagePath + element.ImageCode + '" alt="slider' + index + '" /></div>');
                    imageslist2.push('<div><img src="' + productImagePath + element.ThumbnailCode + '" alt="slider' + index + '" /></div>');

                });

                reCallSlider('.cycle1', imageslist1, data1);
                reCallSlider('.cycle2', imageslist2, data2);

            }

            else {

                $(".leftside").html('<p>No records found.</p>');
                $(".leftside").addClass('no-record');
                $(".cartbox").css('display', 'none');
            }

        });

        CallRelativeProduct = function () {

            Helper.AjaxCall.PostCall('Product/GetRelativeProducts', filters, function (data) {

                var object = JSON.parse(data);
                if (object.IsSuccess) {

                    var productTemplateHTML = $('#RelativeProducts').html();
                    $("#HotDeals434").html('');
                    var iterations = object.ProductList.length < 5 ? object.ProductList.length : 4;

                    for (var i = 0; i < iterations  ; i++) {

                        $("#HotDeals434").append(productTemplateHTML.replace("{{Make}}", object.ProductList[i].Make).
                                                                        replace("%7B%7BStockId%7D%7D", object.ProductList[i].StockId).replace("{{Model}}", object.ProductList[i].Model).
                                                                        replace("{{StockID}}", object.ProductList[i].StockId).
                                                                        replace("{{OldPrice}}", formatter.format(object.ProductList[i].Price)).
                                                                        replace("{{ActualPrice}}", formatter.format(object.ProductList[i].Price)).
                                                                        replace("{{Grade}}", object.ProductList[i].GradeName).
                                                                        replace("{{ProductionYear}}", object.ProductList[i].ProductionYear).
                                                                        replace("{{JapaneseYen}}", japaneseFormat.format(object.ProductList[i].JapaneseYen)).
                                                                        replace("{{ThumbnailImage}}", productImagePath + object.ProductList[i].ProductImage));

                        //$('.rating i').eq(i).css('width', object.ProductList[i].Rating);
                    }
                }
                else {
                    $("#HotDeals434").html('');
                }
            });
        }


    });

</script>


<style type="text/css">
    .cycle-slide img {
        width: 870px;
        height: 532px;
    }
</style>

