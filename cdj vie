<script type="text/javascript">
    $(document).ready(function () {

        var typingTimer;
        var doneTypingInterval = 500;
        var $input = $('#small-searchterms');

        CallStock = function () {

            var selectedBrandValue = $('#instant-search-categories').val() == '' ? '' : (' ' + $('#instant-search-categories').val())
            window.location = '/stock/' + $('#small-searchterms').val() + selectedBrandValue +  '/5';
        }

        $('.search-box-button , #SeeMore').click(function () {
            CallStock();
        });

        function PopulateBrandsWithIcon() {

            var searchBrands = [];
            for (var i = 0; i < BrandsList.length; i++) {

                var objectSet = {
                    label: BrandsList[i].Label,
                    icon: BrandsList[i].Name + ".png",
                    id: BrandsList[i].Id
                }
                searchBrands.push(objectSet);
            }

            autocomp('.programList', searchBrands);
        }

            
        $('.prList').on('keydown', function () {

            alert('Working');

        });


        function PopulateBodyType() {

            for (var i = 0; i < BodyTypeList.length; i++) {

                $('.topmenu-body-type ul').append('<li><a href="/stock/' + BodyTypeList[i].Id + '/1">' + BodyTypeList[i].Label + '</a></li>');
            }

            $('.topmenu-body-type ul').append('<li><a href="/stock">View All</a></li>');
        }

        function PopulateBrands() {

            var count = 0;
            for (var i = 0; i < BrandsList.length; i++) {
                count++;
                if (count > 19) {
                    count = 0;
                    $('.topmenu-brands .brandscope ul').last().append('<li><a href="/stock">View All</a></li>');
                    $('.topmenu-brands .brandscope').append('<ul></ul>');
                }

                else {
                    $('.topmenu-brands .brandscope ul').last().append('<li><a href="/stock/' + BrandsList[i].Id + '/2">' + BrandsList[i].Label + '</a></li>');
                }
            }
            $('.topmenu-brands .brandscope ul').last().append('<li><a href="/stock">View All</a></li>');
        }


        function PopulatePriceRange() {

            for (var i = 0; i < PriceRangeList.length; i++) {
                $('.topmenu-search-by-price ul').append('<li><a href="/stock/' + PriceRangeList[i].Id + '/3">' + PriceRangeList[i].Label + '</a></li>');
            }
            $('.topmenu-search-by-price ul').append('<li><a href="/stock">View All</a></li>');
        }


        function PopulateBrandsDropdown() {

            var option = '';
            for (var i = 0; i < BrandsList.length; i++) {
                option += '<option value="' + BrandsList[i].Label + '">' + BrandsList[i].Label + '</option>';
            }
            $('#instant-search-categories').append(option);
        }


        PopulateBrandsWithIcon();
        PopulateBodyType();
        PopulateBrands();
        PopulatePriceRange();
        PopulateBrandsDropdown();

        $input.on('keyup', function (event) {

            if (ValidateInput(event)){
                clearTimeout(typingTimer);
                typingTimer = setTimeout(GetSearchedRecords, doneTypingInterval);
            }
        });

        //on keydown, clear the countdown
        $input.on('keydown', function (event) {

            if (ValidateInput(event)) {
                clearTimeout(typingTimer);
            }
        });


        $input.on('keypress', function (evt) {

            var charCode = (evt.which) ? evt.which : evt.keyCode

            if (charCode == 60 || charCode == 62) {
                return false;
            }
            return true;
        });

        $('#searchForm').on('submit', function () {

            CallStock();
            return false;
        });

        function ValidateInput(evt) {

            if ($('#small-searchterms').val().length < 3) {
                return false;
            }

            else {
                return true;
            }

        }

        function GetSearchedRecords() {

            Helper.AjaxCall.GetCall($('#searchForm').attr('action'), $('#searchForm').serialize(), function (object) {

                var data = JSON.parse(object);

                if (data.IsSuccess && data.TotalRecords > 0) {

                    $('#small-searchterms-list').css('display', 'block');
                    $('#SeeMore').css('display', 'block');

                    var productTemplateHTML = $('#searchedProductTemplate').html();
                    $(".k-list").html('');
                    for (var i = 0; i < data.ProductList.length; i++) {

                        $(".k-list").append(productTemplateHTML.replace("{{Make}}", data.ProductList[i].Make).
                                                                        replace("%7B%7BStockId%7D%7D", data.ProductList[i].StockId).
                                                                        replace("{{StockID}}", data.ProductList[i].StockId).
                                                                        replace("{{Model}}", data.ProductList[i].Model).
                                                                        replace("{{Price}}", formatter.format(data.ProductList[i].Price)).
                                                                        replace("{{ProductionYear}}", data.ProductList[i].ProductionYear).
                                                                        replace("{{ProductImage}}", productImagePath + data.ProductList[i].ProductImage));
                    }
                }
                else {

                    $('#small-searchterms-list').css('display', 'none');

                    $(".k-list").html('<li>No records found</li>');
                }

                $('.search_popup').css("display", "block").css("width", "100%");

            });
        }

        $('.phoneinput').intlTelInput({
            initialCountry: "auto",
            utilsScript: "/Scripts/utils.js"
        });

        $('#headerCountry').on("countrychange", function (e, countryData) {
            Helper.Methods.SetCookie("countrysearch", countryData.iso2.toUpperCase(), 86400); /*1 day expiry*/
            $('.SearchCountryFlag').val(countryData.iso2);
        });

        var searchcountry = Helper.Methods.GetCookie("countrysearch");
        if (searchcountry != null) {
            $('#headerCountry').intlTelInput("setCountry", searchcountry);
        }

    });
</script>



<script id="searchedProductTemplate" type="text/html">

    <li class="k-item">
        <a class="" style="border-bottom:none; margin:0px; padding:0px;" href="@Url.Action("ProductDetails", "Product", new { Id = "{{StockId}}" })">
            <div class="instant-search-item">
                <div class="img-block"><img src="{{ProductImage}}" alt="" style="width:62px; height:62px;" title=""></div>
                <div class="detail">
                    <div class="title">{{Make}} {{Model}} {{ProductionYear}}</div>
                    <div class="title">{{StockID}}</div>
                    <div class="price">{{Price}}</div>
                    @*<button class="addtocart addtocart3">Add to Cart</button>*@
                </div>
            </div>
        </a>
    </li>

</script>



