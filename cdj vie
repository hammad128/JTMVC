+ Index.cshtml

@{
    ViewBag.Title = "Stock";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

<section class="content-inner2 multi-category">
    <h2 style="display: none;">&nbsp;</h2>
    <article>
        <div class="sidebar productsearch productBar">
            <h5>Category <span>Filter</span></h5>
            <strong class="Brands">
                <a href="javascript:;" title="Cisco">Body Type</a>
            </strong>
            <ul class="body-list-type"></ul>

            <strong class="Brands">
                <a href="javascript:;" title="brands">Brands</a>
            </strong>
            <ul class="brand-list-type"></ul>

            <strong class="Brands">
                <a href="javascript:;" title="">Search By Price</a>
            </strong>
            <ul class="price-list-type"></ul>

            <strong class="Brands">
                <a href="javascript:;" title="">Search By Year</a>
            </strong>
            <ul class="year-list-type"></ul>
        </div>
        <div class="rightside">
            
            <div class="gridbox">
                <div class="gridhead">
                    <h3 class="results">Showing <span class="resultsrange">0</span> of <span class="totalrecords">0</span>  Results</h3>
                    <ul>
                        <li>Sort By:</li>
                        <li class="select">
                            <select id="products-orderby2" name="products-orderby2" onchange="changeOrderBy(this.value);">
                                <option selected="selected" value="0">Name: A to Z</option>
                                <option value="1">Name: Z to A</option>
                                <option value="2">Price: Low to High</option>
                                <option value="3">Price: High to Low</option>
                            </select>
                        </li>
                        <li class="grid"><a href="javascript:;" title="" class=""></a></li>
                        <li class="list"><a href="javascript:;" title="" class="active"></a></li>
                    </ul>
                </div>

                <div class="shopbycategory"><ul class="gridbody"></ul></div>

                <div class="pager">
                    <div class="gridfooter">
                        <ul>
                            <li class="results">Showing <span class="resultsrange">0</span> of <span class="totalrecords">0</span> Results</li>
                            <li class="pagers">
                                <ul></ul>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>            
        </div>
    </article>
</section>


<script>

    $(document).ready(function () {

        var CurrentPage = 1;
        var FilterValue;
        var FilterCategoryId;
        var totalPage;
        var pageSize = 40;
        var filters = {};

        changeOrderBy = function(selectedValue){

            ProductByType(FilterValue,FilterCategoryId,CurrentPage,selectedValue);
        }


        Helper.AjaxCall.GetCall('ProductAttribute/GetBodyType', {}, function (data) {
            var object = JSON.parse(data);
            if (object.IsSuccess) {
                for (var i = 0; i < object.ValueModel.length; i++) {
                    $('.body-list-type').append('<li><a href="javascript:;" onclick=ProductByType(' + object.ValueModel[i].Id + ',1,1) >' + object.ValueModel[i].Name + '</a></li>');
                }
            }
        });


        Helper.AjaxCall.GetCall('ProductAttribute/GetBrands', {}, function (data) {

            var object = JSON.parse(data);
            if (object.IsSuccess) {
                for (var i = 0; i < object.ValueModel.length; i++) {
                    $('.brand-list-type').append('<li><a href="javascript:;" onclick=ProductByType(' + object.ValueModel[i].Id + ',2,1) >' + object.ValueModel[i].Name + '</a></li>');
                }
            }
        });

        Helper.AjaxCall.GetCall('ProductAttribute/GetPriceRange', {}, function (data) {

            var object = JSON.parse(data);
            if (object.IsSuccess) {
                for (var i = 0; i < object.ValueModel.length; i++) {

                    $('.price-list-type').append('<li><a href="javascript:;" onclick=ProductByType(' + object.ValueModel[i].Id + ',3,1) >' + object.ValueModel[i].Name + '</a></li>');
                }
            }
        });


        function GetCarsByProductionDate(){

            var searchcountry = Helper.Methods.GetCookie("countrysearch");
            filters.CountryFlag = searchcountry != null ? searchcountry : "";

            Helper.AjaxCall.GetCall('ProductAttribute/GetCarsByProductionDate', filters, function (data) {

                var object = JSON.parse(data);
                if (object.IsSuccess) {
                    
                    $('.year-list-type').html('');
                    for (var i = 0; i < object.ValueModel.length; i++) {
                        $('.year-list-type').append('<li><a href="javascript:;" onclick=ProductByType(' + object.ValueModel[i].Id + ',4,1) >' + object.ValueModel[i].Name + '</a></li>');
                    }
                }
            });
        }
         
        GetCarsByProductionDate();

        $(".pagers ul").on("click", ".rightnav", function () {
            ProductByType(FilterValue, FilterCategoryId, CurrentPage + 1);
        });

        $(".pagers ul").on("click", ".leftnav", function () {
            ProductByType(FilterValue, FilterCategoryId, CurrentPage - 1);
        });


        $('#headerCountry').on("countrychange", function (e, countryData) {
            ProductByType(0, 0, 1);
            GetCarsByProductionDate();

        });

        $('.body-list-type , .brand-list-type , .price-list-type , .year-list-type').on('click' , 'li a' , function(){
        
            $(this).closest('.productsearch').find('li a.active').removeClass('active');
            $(this).addClass('active');
        
        }); 

        ProductByType = function (filterValue, filterCategoryId , currentPage) {

            CurrentPage = currentPage;
            FilterValue = filterValue;
            FilterCategoryId = filterCategoryId;

            var filters = ActivateFilter(filterValue, filterCategoryId , currentPage);


            Helper.AjaxCall.PostCall('Product/GetProducts', filters, function (data) {

                var object = JSON.parse(data);
                if (object.IsSuccess) {

                    $("html, body").animate({ scrollTop: 0 }, "slow");

                    var count = 0
                    var productTemplateHTML = $('#productTemplate').html();
                    $(".shopbycategory .gridbody").html('');

                    for (var i = 0; i < object.ProductList.length; i++) {

                        $(".shopbycategory .gridbody").append(productTemplateHTML.replace("{{Make}}", object.ProductList[i].Make).
                                                                        replace("{{Model}}", object.ProductList[i].Model).
                                                                        replace("{{Year}}", object.ProductList[i].ProductionYear).
                                                                        replace("{{StockId}}", object.ProductList[i].StockId).
                                                                        replace("{{Price}}", formatter.format(object.ProductList[i].Price)).
                                                                        replace("{{className}}", i % 2 ? "addsep" : "").
                                                                        replace("{{JapaneseYen}}", japaneseFormat.format(object.ProductList[i].JapaneseYen)).
                                                                        replace("{{Image}}", productImagePath + object.ProductList[i].ProductImage).
                                                                        replace("%7B%7BStockId%7D%7D", object.ProductList[i].StockId));
                    }

                    $(".gridfooter .pagers ul").html('');
                    var paginationTemplate = $('#paginationTemplate').html();

                    $(".totalrecords").text(object.TotalRecords);
                    var from = (currentPage - 1) * pageSize + 1;
                    var to = (from + object.ProductList.length - 1);
                    $(".resultsrange").text(from + " - " + to);
                    $(".gridfooter .pagers ul").append('<li class="leftnav" style=' + (currentPage == 1 ? "pointer-events:none;opacity:0.4;" : "") + '><a href="javascript:;">&gt;</a></li>');
                    totalPage = Math.ceil(object.TotalRecords / pageSize);

                    var endIndex = totalPage;
                    var startIndex = 1;

                    if (totalPage >= 5)
                    {
                        startIndex = currentPage - 1;
                        endIndex = currentPage + 3;

                        if (startIndex <= 0) {
                            startIndex = 1;
                            endIndex = startIndex + 4;
                        }

                        if (endIndex > totalPage) {
                            endIndex = totalPage;
                            startIndex = totalPage - 4;
                        }
                    }

                    for (var i = startIndex ; i <= endIndex ; i++) {

                        $(".gridfooter .pagers ul").append(paginationTemplate.replace("{{FilterValue}}", filterValue).
                                                                              replace("{{FilterCategoryId}}",FilterCategoryId).
                                                                              replace("{{i}}",i).replace("{{i}}",i).
                                                                              replace("{{anchorClassName}}" , i == CurrentPage ? "active" : ""));
                    }

                    $(".gridfooter .pagers ul").append('<li class="rightnav" style=' + (currentPage  == endIndex ? "pointer-events:none;opacity:0.4;" : "") + '  ><a href="javascript:;">&gt;</a></li>');

                    setTimeout(function () {
                        $("img.lazy").lazyload();
                    }, 1000);
                }

                else {
                    
                    $(".shopbycategory .gridbody").html('<li>No records found.</li>');
                    $(".shopbycategory .gridbody li").addClass('no-record');
                    $(".pagers ul").html('');
                    $(".totalrecords").text(0);
                    $(".resultsrange").text(0);
                }

            });
        }

        ActivateFilter = function (filterValue, filterCategoryId, currentPage) {

            var searchcountry = Helper.Methods.GetCookie("countrysearch");            
            var filters = {};
            filters.CountryFlag = searchcountry != null ? searchcountry : "";
            filters.PageSize = pageSize;
            filters.PageNumber = currentPage;
            filters.OrderID = $("#products-orderby2").val();
            $('#small-searchterms').val('');


            if (filterCategoryId == 1) {
                filters.BodyTypeId = filterValue;
            }

            else if (filterCategoryId == 2) {
                filters.MakeId = filterValue;
            }

            else if (filterCategoryId == 3) {
                filters.PriceId = filterValue;
            }

            else if (filterCategoryId == 4) {
                filters.YearId = filterValue;
            }

            else if(filterCategoryId == 5){
                filters.SearchTerm = filterValue;
                $('#small-searchterms').val(filterId);
            }

            return filters;

        }

        var filterId = '@ViewBag.FilterId';
        var filterCategoryId = '@ViewBag.FilterCategoryId';
        ProductByType(filterId , filterCategoryId , 1);
    });




</script>

<style type="text/css">

    .multi-category .gridbody.listing li span.rte {
        margin-top: -42px;
        margin-right:21px;
    }

</style>


<script id="productTemplate" type="text/html">

        <li class="product-item {{className}}">
            <div class="dtbl">
                <div class="dtd wd-px190">
                    <div class="text-wrap">
                        <a class="head" href="@Url.Action("ProductDetails", "Product", new { Id = "{{StockId}}" })"><h5>{{Make}} {{Model}} - {{Year}}</h5></a>
                        <p>StockId:<span>&nbsp;{{StockId}}</span></p>
                        <span class="rte">{{JapaneseYen}}</span><br/>
                        <span class="rte" style="margin-top:-15px; margin-right:-63px;">{{Price}}</span>
                        @*<a href="@Url.Action("ProductDetails", "Product", new { Id = "{{ProductId}}" })">View</a>*@
                    </div>
                </div>
                <div class="dtd wd-px240">
                    <img class="lazy" src="/Content/images/comingsoon_renew.png" data-original="{{Image}}" alt="" width="201" height="151">
                </div>
            </div>
        </li>
    
</script>


<script id="paginationTemplate" type="text/html">
    <li class="individual-page"><a class="{{anchorClassName}}" href="javascript:;" onclick="ProductByType({{FilterValue}} , {{FilterCategoryId}} ,{{i}})">{{i}}</a></li>
</script>


