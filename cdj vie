<script type="text/javascript">
    $(document).ready(function () {
        $('body').addClass("contact-us-map");

        $('[id*=txtPhone]').intlTelInput({
            initialCountry: "auto",
            utilsScript: "/Scripts/utils.js"
        });

        $('[id*=txtPhone]').on("countrychange", function (e, countryData) {
            var countryData = $('[id*=txtPhone]').intlTelInput("getSelectedCountryData");
            $('[id*=hfCellFlag]').val(countryData.iso2);
        });

        $('[id*=txtPhone]').keydown(function (e) {
            if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 110, 190]) !== -1 ||
                (e.keyCode === 65 && (e.ctrlKey === true || e.metaKey === true)) ||
                (e.keyCode >= 35 && e.keyCode <= 40)) {
                return;
            }
            if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
                e.preventDefault();
            }
        });

        $('[id*=btnSubmit]').click(function () {
            return ValidateForm();
        });

        var country = Helper.Methods.GetCookie("country");
        if (country != null) {
            $('[id*=txtPhone]').intlTelInput("setCountry", country);
        }
    });
    function ValidateForm() {
        var iserror = false;
        var name = $('[id*=txtName]');
        var email = $('[id*=txtEmail]');
        var phone = $('[id*=txtPhone]');        

        name.removeClass('error');
        email.removeClass('error');
        phone.removeClass('error');        

        if (name.val().trim() == '' || typeof name.val() == "undefined") {
            name.addClass('error');
            iserror = true;
        }
        if (email.val().trim() == '' || typeof email.val() == "undefined" || !Helper.Methods.ValidateEmail(email.val().trim())) {
            email.addClass('error');
            iserror = true;
        }
        if (phone.val().trim() == '' || typeof phone.val() == "undefined") {
            phone.addClass('error');
            iserror = true;
        }        
        return !iserror;
    }

</script>

+ HeaderAccount.cshtml

@model CarDealsJapanCore.Models.Customer.CustomerInfo


@if (Model.UserName != null && !String.IsNullOrEmpty(Model.UserName))
{

    <li class="droplist signinddm">
        Hello, @Model.UserName your<strong> account</strong>
        <ul class="showMenu">
            <li>
                <div class="row">
                    <h2 style="line-height:normal; word-wrap:break-word;">Hello, @Model.UserName</h2>
                </div>
                <a href='@( Url.Action("Logout", "Account"))' class="yellowBtn signin-btn">Sign Out</a>

                <ul>
                    <li><a title="" href='@( Url.Action("AccountInfo", "Account"))'>Customer info</a></li>
                    <li><a title="" href='@( Url.Action("OrderHistory", "Account"))'>Orders</a></li>
                    <li><a title="" href='@( Url.Action("ChangePassword", "Account"))'>Change password</a></li>
                </ul>
            </li>
        </ul>
    </li>
}
else
{
    <li class="droplist signinddm">
        Hello, Sign in to your <strong>account</strong>
        <ul class="showMenu">
            <li>
                <a href='@( Url.Action("Login", "Account"))' class="yellowBtn signin-btn">Sign In</a>
                <div class="row"><span>New customer?</span> <a title="" href='@( Url.Action("Register", "Account"))'>Start here.</a></div>
                <ul>
                    <li><a title="" href='@( Url.Action("AccountInfo", "Account"))'>Customer info</a></li>
                    <li><a title="" href='@( Url.Action("OrderHistory", "Account"))'>Orders</a></li>
                    <li><a title="" href='@( Url.Action("ChangePassword", "Account"))'>Change password</a></li>
                </ul>
            </li>
        </ul>
    </li>
}



